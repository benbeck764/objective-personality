{
  "name": "objective-personality",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benbeck764/objective-personality.git"
  },
  "bugs": {
    "url": "https://github.com/benbeck764/objective-personality/issues"
  },
  "homepage": "https://github.com/benbeck764/objective-personality#readme",
  "author": "Ben Beck",
  "license": "ISC",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev:app": "pnpm build:data && pnpm --filter objective-personality-app dev",
    "dev:function": "pnpm build:data && pnpm --filter objective-personality-etl-function start",
    "build": "pnpm --filter objective-personality-data build && pnpm --filter objective-personality-app build && pnpm --filter objective-personality-etl-function build",
    "build:app": "pnpm --filter objective-personality-data build && pnpm --filter objective-personality-app build",
    "build:function": "pnpm --filter objective-personality-data build && pnpm --filter objective-personality-etl-function build",
    "build:data": "pnpm --filter objective-personality-data build",
    "clean": "pnpm -r clean",
    "clean:app": "pnpm --filter objective-personality-app clean",
    "clean:function": "pnpm --filter objective-personality-etl-function clean",
    "generate": "pnpm --filter objective-personality-data generate",
    "generate:app": "pnpm --filter objective-personality-app generate",
    "generate:function": "pnpm --filter objective-personality-etl-function generate",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "@types/node": "^18.x",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2"
  }
}
