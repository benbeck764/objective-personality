    "@azure/storage-blob": "^12.17.0",
    "objective-personality-data": "workspace:*",
    "axios": "^1.5.0",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "main": "dist/src/functions/*.js"
}
