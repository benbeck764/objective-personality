{
  "name": "objective-personality-data",
  "version": "1.0.0",
  "description": "Prisma data layer for OP app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benbeck764/objective-personality.git",
    "directory": "data"
  },
  "bugs": {
    "url": "https://github.com/benbeck764/objective-personality/issues"
  },
  "homepage": "https://github.com/benbeck764/objective-personality#readme",
  "license": "ISC",
  "engines": {
    "node": ">=18.18.0"
  },
  "main": "dist/node/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/node/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./src/generated/client/index.js",
      "require": "./src/generated/client/index.js",
      "types": "./src/generated/client/index.d.ts"
    }
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "build": "pnpm generate && pnpm build:esm && pnpm build:common",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:common": "tsc --project tsconfig.common.json",
    "clean": "rimraf node_modules",
    "clean:build": "rimraf dist",
    "generate": "prisma generate --schema=src/prisma/schema.prisma",
    "migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma"
  },
  "peerDependencies": {
    "@prisma/adapter-mssql": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "mssql": "^12.0.0"
  },
  "devDependencies": {
    "@prisma/adapter-mssql": "^7.3.0",
    "@prisma/client": "7.2.0",
    "@types/mssql": "^9.1.9",
    "@types/node": "^18.x",
    "mssql": "^12.2.0",
    "prisma": "7.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2"
  }
}
